{
  "name": "api-clients/github",
  "description": "Async first Github client",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "api-clients/openapi-client-generator": "dev-master",
    "wyrihaximus/hydrator": "dev-master",
    "cebe/php-openapi": "dev-webhooks as 1.6.0",
    "league/openapi-psr7-validator": "dev-webhooks as 0.16.99"
  },
  "autoload": {
    "psr-4": {
      "ApiClients\\Client\\Github\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApiClients\\Tests\\Client\\Github\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:WyriHaximus-labs/php-openapi.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:WyriHaximus-labs/openapi-psr7-validator.git"
    }
  ],
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.4.7"
    }
  },
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^3.4"
  }
}
